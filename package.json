{
	"name": "nest-template",
	"version": "1.0.0",
	"description": "",
	"author": "",
	"private": true,
	"license": "UNLICENSED",
	"engines": {
		"node": ">=20",
		"pnpm": ">=9.0.0"
	},
	"scripts": {
		"prebuild": "rimraf dist",
		"build": "nest build",
		"prebundle": "rimraf build && npm run build",
		"bundle": "ncc build --no-cache --target es2015 dist/main.js -o build -m",
		"postbundle": "node ./scripts/postbundle.js",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"dev": "nest start --watch",
		"docker:dev": "docker-compose -f docker-compose.dev.yaml up -d",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "NODE_ENV=e2e jest --runInBand --config ./test/jest-e2e.json"
	},
	"dependencies": {
		"@nestjs/cli": "^11.0.2",
		"@nestjs/common": "^11.0.4",
		"@nestjs/config": "^4.0.0",
		"@nestjs/core": "^11.0.4",
		"@nestjs/jwt": "^11.0.0",
		"@nestjs/mapped-types": "2.1.0",
		"@nestjs/passport": "^11.0.5",
		"@nestjs/platform-express": "^11.0.4",
		"@nestjs/swagger": "^11.0.2",
		"@prisma/client": "^5.10.0",
		"bcryptjs": "^2.4.3",
		"chalk": "^4.1.2",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.1",
		"lodash": "^4.17.21",
		"nestjs-i18n": "^10.5.0",
		"passport": "^0.7.0",
		"passport-jwt": "^4.0.1",
		"reflect-metadata": "^0.2.2",
		"remove": "^0.1.5",
		"rimraf": "^6.0.1",
		"rxjs": "^7.8.1",
		"swagger-ui-express": "^5.0.1",
		"typescript": "^5.7.3"
	},
	"devDependencies": {
		"@nestjs/schematics": "^11.0.0",
		"@nestjs/testing": "^11.0.4",
		"@types/bcryptjs": "^2.4.6",
		"@types/express": "^5.0.0",
		"@types/jest": "29.5.14",
		"@types/lodash": "^4.17.14",
		"@types/node": "^20.17.16",
		"@types/passport-jwt": "^4.0.1",
		"@types/supertest": "^6.0.2",
		"@typescript-eslint/eslint-plugin": "^8.21.0",
		"@typescript-eslint/parser": "^8.21.0",
		"@vercel/ncc": "^0.38.3",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-prettier": "^5.2.3",
		"jest": "29.7.0",
		"prettier": "^3.4.2",
		"prisma": "^5.10.0",
		"source-map-support": "^0.5.21",
		"supertest": "^6.3.4",
		"ts-jest": "29.2.5",
		"ts-loader": "^9.5.2",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "4.2.0"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node",
		"moduleNameMapper": {
			"^@/(.*)$": "<rootDir>/../src/$1"
		}
	}
}